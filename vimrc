set encoding=utf-8
set number
set nocompatible
set autoindent
set tabstop=2
set shiftwidth=2
set expandtab
set softtabstop=2
set cursorline
set textwidth=80
set wrap
set linebreak
set wrapmargin=2

call plug#begin('~/.vim/plugged')
Plug 'jiangmiao/auto-pairs'
Plug 'preservim/nerdtree'
call plug#end()
